ST($VX_0)
(*** END AUXILIARY RUNG ***)
(*** BEGIN MAIN RUNG ***)
LD($VX_0)
LDN(Desliga)
ANDLD(K4)
ORS
LDN(Q3)
ANDLDN(K1)
